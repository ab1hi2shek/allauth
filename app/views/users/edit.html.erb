

<%= form_for(@user) do |f| %>
<br/>
<div class="container">
	<div class="row">
	<div class="col-md-3">
	</div>
	<div class="col-md-6">

	<div class="panel panel-warning">
    	<div class="panel-heading"><h3 style="text-align: center">Edit Your Profile
 	<span style="float:right"><div class="btn-group"><%= link_to "Back", root_path, class:"btn btn-primary" %><%= f.submit "Update", class: 'btn btn-success' %></span></div></h3>
 	</div>
        <div class="panel-body">
        	<div class="form-group">
        	<%= f.label :name %>
			<%= f.text_field :name, class: 'form-control' %>
			</div>

<br />

<div class="form-group">
            <%= f.label :Email %>
            <%= f.text_field :email, class: 'form-control' %>
            </div>

<br />
<br />
        </div>
 	</div>
 	</div>

<div class="col-md-3">
	</div>


</div>
</div>

  <script type="text/javascript">
            $(function () {
                $('#datetimepicker1').datetimepicker(
{	format: 'DD/MM/YYYY'}
                	);
            });
        </script>



<% end %>

